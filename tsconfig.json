
{
    "compilerOptions": {
      "jsx": "react",
      "target": "es6",
      "module": "commonjs",
      "lib": [
        "DOM",
        "ESNext",
        "es6",
   
      ],
      "allowJs": true,
      "checkJs": true,
      "declaration": true,
      "emitDeclarationOnly": true,
      "declarationMap": true,
      "declarationDir": "./dist",
      "noEmit": false,
      "strict": true,
      "suppressImplicitAnyIndexErrors": true,
      "noImplicitThis": false,
      "moduleResolution": "node",
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true
    },
    "include": [
      "src"
    ],
   
  }
